<script type="text/javascript">
      $(".sunday").hide();

     $(document).ready(function(){
        $('input[type="checkbox"]').click(function(){
            if($(this).attr("value")=="sun"){
                $(".sunday").toggle(500);
            }
        });
    });


</script>
